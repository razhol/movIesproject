<body>
  <ul id="list">
    <li>Id: <%=data.id %>
    </li>
    <li>Name: <%= data.name %>
    </li>
    <li>Language:<%= data.language %>
    </li>
    <li>
      <ul>Geners: <% data.genres.forEach(function(gener){ %>
          <%= gener %> ,
            <% }) %>
      </ul>
