    <li>
      <% if(( 'image' in data)){ %>
        <p>picture movie:</p>
        <img src="<%= data.image.medium %>">
        <% } %>
    </li>
  </ul>
  <a href="/searchmovie/Searchres">back</a> <br />
  <a href="/menu">back to menu</a>
</body>

</html>